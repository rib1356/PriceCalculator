{
  "rules": {
    ".read": true,
    ".write": true,
    "GPM": {  
    	"$GPMId": {
        "gpm": {
         	".validate": "newData.isNumber() && newData.val() <= 100" 
        },
        "rowMin": {
          ".validate": "newData.isNumber()"
        },
        "rowMax": {
          ".validate": "newData.isNumber()"
        }  
    	}
    }
  }
}